  <h1>New book</h1>
  
   <% @lists.each do |list| %>
     <p>Title: <%= list.title %></p>
     <p>Body: <%= list.body %></p>
   <% end %>

   <%= form_with model: @list,url: '/lists',methood: :post do |f| %>
     <h4>Title</h4>
     <%= f.text_field :title %>
     
     <h4>Body</h4>
     <%= f.text_area :body %>

     <%= f.submit 'Create books' %>
   <% end %>
   